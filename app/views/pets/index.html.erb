<h1>Pets#index</h1>

<ul>
  <% @pets.each do |pet| %>
    <li id=pet-<%= pet.id %>>
      <span>Image: <%= pet.image %></span>
      <span>Name: <%= pet.name %></span>
      <span>Age: <%= pet.age %></span>
      <span>Sex: <%= pet.sex %></span>
      <span>Shelter: <%= pet.shelter.name %></span>
      <%= link_to 'edit', "/pets/#{pet.id}/edit" %>
      <%= link_to 'delete', "/pets/#{pet.id}", method: :delete %>
    </li>
  <% end %>
</ul>
